if [ ! -f ".env" ]; then
  echo "NOTE: Missing .env environment file. Copying env.example to .env -- please edit accordingly."
  cp env.example .env
fi

NETWORK=`docker network ls | grep 'wirestarter'`
if [ -z "$NETWORK" ]
then
  docker network create --attachable wirestarter --subnet 172.50.0.1/24
fi

COMPOSE_IGNORE_ORPHANS=true docker-compose -f docker/docker-compose.yml --env-file .env --project-name wirestarter "$@"